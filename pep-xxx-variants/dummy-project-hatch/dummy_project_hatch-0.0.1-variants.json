{
    "$schema": "https://variants-schema.wheelnext.dev/v0.0.3.json",
    "default-priorities": {
        "namespace": [
            "fictional_hw",
            "fictional_tech"
        ]
    },
    "providers": {
        "fictional_hw": {
            "plugin-api": "provider_fictional_hw.plugin:FictionalHWPlugin",
            "requires": [
                "provider-fictional-hw == 1.0.0"
            ]
        },
        "fictional_tech": {
            "plugin-api": "provider_fictional_tech.plugin:FictionalTechPlugin",
            "requires": [
                "provider-fictional-tech == 1.0.0"
            ]
        }
    },
    "variants": {
        "foobar": {
            "fictional_hw": {
                "architecture": [
                    "mother"
                ]
            }
        },
        "foobar2": {
            "fictional_hw": {
                "architecture": [
                    "mother"
                ],
                "compute_accuracy": [
                    "10"
                ]
            }
        }
    }
}